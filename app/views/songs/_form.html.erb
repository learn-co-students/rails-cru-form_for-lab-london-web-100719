<h3>Song Form</h3>
<br>
<%= form_for @song do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>

    <%= f.label "Artist ID" %>
    <%= f.text_field :artist_id %>
    <br>

    <%= f.label "Genre ID" %>
    <%= f.text_field :genre_id %>
    <br>

    <!--
    <%= f.collection_select :genre_id,  Genre.all, :id, :name, include_blank: true %>
    <br>

    <%= f.collection_select :artist_id, Artist.all, :id, :name, include_blank: true %>
    <br>
    -->

    <%= f.submit %>
<% end %>